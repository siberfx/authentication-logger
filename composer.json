{
    "name": "siberfx/authentication-logger",
    "description": "Log user authentication details and send new device notifications.",
    "keywords": [
        "Siberfx",
        "laravel",
        "authentication-logger"
    ],
    "homepage": "https://github.com/siberfx/authentication-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Selim Gormus",
            "email": "info@siberfx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Siberfx\\AuthenticationLogger\\": "src",
            "Siberfx\\AuthenticationLogger\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siberfx\\AuthenticationLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Siberfx\\AuthenticationLogger\\AuthenticationLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
